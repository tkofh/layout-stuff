{
  "name": "nuxt-app",
  "packageManager": "pnpm@9.15.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "format": "prettier --write .",
    "lint:ts": "eslint --ext .js,.mjs,.ts,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint --ignore-path .gitignore --fix \"**/*.{css,scss,vue}\"",
    "lint": "pnpm lint:ts && pnpm lint:css",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/eslint": "0.7.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@radix-ui/colors": "^3.0.0",
    "@vue/language-server": "^2.1.10",
    "eslint": "^9.17.0",
    "nuxt": "^3.14.1592",
    "nuxt-security": "2.1.5",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "radix-vue": "^1.9.11",
    "sanitize.css": "^13.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-plugin-logical-css": "^1.2.1",
    "typescript": "^5.7.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "glob": "7.2.3",
      "inflight": "1.0.6"
    }
  },
  "dependencies": {
    "@vueuse/core": "^12.2.0",
    "@vueuse/nuxt": "^12.2.0"
  }
}
